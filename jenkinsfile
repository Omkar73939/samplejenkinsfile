pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing ... '
            }
        }
        stage('Deploye') {
            steps {
                echo 'Deploying..,'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing..,'
            }
        }
      
    }
}
